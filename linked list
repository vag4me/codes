#include <stdio.h>

struct entry
{
    int value;
    struct entry *next;
};


int main(void)
{
    struct entry n1,n2,n3;
    int *head,*tail;
    
    head=&n1.next;
    tail=&n3.next;
    tail=NULL;
    
    n1.value=100;
    n2.value=200;
    n3.value=300;
    
    n1.next=&n2;
    n2.next=&n3;
    
    printf("%d\n",n1.value);
    printf("%d\n",n1.next->value);
    printf("%d\n",n2.next->value);
    return 0;
}
