
#include <stdio.h>

struct student
{
    int age;
    int grade;
};

struct student getinfo(struct student s2)
{
    scanf("%d",&s2.age);
    scanf("%d",&s2.grade);
    return s2;
}

int main()
{
    struct student s1;
    
    s1 = getinfo(s1);
    
    printf("%d %d",s1.age,s1.grade);

    return 0;
}
